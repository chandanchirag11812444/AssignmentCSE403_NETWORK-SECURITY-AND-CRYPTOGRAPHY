from distutils.util import run_2to3
from math import gcd

# a=input("enter the first number ")
# a=int(a)
# b=input("enter the second number ")
# b=int(b)
a=112
b=70
r1 = a
r2 = b
s1 = 1
s2 = 0
t1 = 0
t2 = 1
r = 0
s = 0
t = 0
while(r2>0):
    q=r1//r2
    r=r1-q*r2
    r1=r2
    r2=r


    s=s1-q*s2
    s1=s2
    s2=s

    t=t1-q*t2
    t1=t2
    t2=t

gcd=r1
s=s1
t=t1

print("the gcd of a",a," and ",b ,"is ",gcd)
print("the value of s is ",s)
print("the value of t is ",t)



